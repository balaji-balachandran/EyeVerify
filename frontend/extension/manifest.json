{
    "manifest_version": 3,
    "name": "EyeVerify",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "./popup/popup.html",
      "default_icon": "./images/eyelogo.png"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],
    "permissions": ["activeTab", "scripting"]
  }